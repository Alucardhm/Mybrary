<h2>Search Authors</h2>

<form action="/authors" method="GET"> <!-- AUTHORS INDEX IS A GET ROUTE -->
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% authors.forEach(author => {%>
    <div><%= author.name%></div>
<% }) %>